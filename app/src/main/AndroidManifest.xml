<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chewbs.alloheroproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UserSigninActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserSignupActivity" />
        <activity android:name=".AssociationSigninActivity" />
        <activity android:name=".AssociationSignupActivity" />
        <activity android:name=".AssociationHomeActivity" />
        <activity android:name=".UserHomeActivity" />
        <activity android:name=".UserAssociationListActivity" />
        <activity android:name=".AssociationSelectedLocationActivity" />
        <activity android:name=".AssociationSelectedUserActivity" />
        <activity android:name=".UserSelectedAssociationActivity"></activity>
    </application>

</manifest>